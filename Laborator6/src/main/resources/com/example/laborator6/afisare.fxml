<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<StackPane prefHeight="350.0" prefWidth="489.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.laborator6.AfisarePaginataController">
    <children>
        <VBox alignment="CENTER" spacing="10.0">
            <TableView fx:id="tableView" prefHeight="300.0" prefWidth="414.0">
                <columns>
                    <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="ID" />
                    <TableColumn fx:id="tableColumnNume" prefWidth="75.0" text="Nume" />
                    <TableColumn fx:id="tableColumnPrenume" prefWidth="75.0" text="Prenume" />
                    <TableColumn fx:id="tableColumnData" prefWidth="75.0" text="Data" />
                </columns>
            </TableView>
            <HBox alignment="CENTER" spacing="20.0">
                <Button fx:id="prevButton" mnemonicParsing="false" onAction="#onPreviousPage" prefHeight="26.0" prefWidth="90.0" text="Previous" />
                <Label fx:id="pageNumber" text="Page 0/0" />
                <Button fx:id="nextButton" mnemonicParsing="false" onAction="#onNextPage" prefHeight="26.0" prefWidth="98.0" text="Next" />
            </HBox>
        </VBox>
    </children>
</StackPane>
